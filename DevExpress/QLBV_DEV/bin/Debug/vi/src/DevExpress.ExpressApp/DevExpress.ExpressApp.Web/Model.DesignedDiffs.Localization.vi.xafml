<?xml version="1.0" encoding="utf-8"?>
<Application>
  <ActionDesign>
    <Actions>
      <Action Id="ChooseTheme" Caption="Chọn Chủ đề" />
      <Action Id="Edit" Caption="Sửa" />
      <Action Id="InlineEdit" Caption="Sửa Tại chỗ" />
      <Action Id="NavigateTo" Caption="Điều hướng đến" />
      <Action Id="QuickCreateAction" Caption="Tạo nhanh" />
      <Action Id="SwitchToEditMode" Caption="Sửa" />
    </Actions>
  </ActionDesign>
  <BOModel>
    <Class Name="DevExpress.ExpressApp.Web.LogonAttemptsAmountedToLimit" Caption="Số lần đăng nhập lên đến giới hạn" />
    <Class Name="DevExpress.ExpressApp.Web.ParameterlessLogonFailedInfo" Caption="Thông tin Đăng nhập Phi tham số Lỗi">
      <OwnMembers>
        <Member Name="Message" Caption="Thông báo" />
      </OwnMembers>
    </Class>
  </BOModel>
  <Localization>
    <LocalizationGroup Name="Messages">
      <LocalizationItem Name="ThereAreNoRecordsToDisplay" Value="Không có bản ghi nào để hiển thị." />
    </LocalizationGroup>
  </Localization>
  <Views>
    <DetailView Id="AboutInfo_DetailView">
      <Items>
        <StaticText Id="AboutText" Text="{0:ProductName}&lt;br&gt;{0:Version}&lt;br&gt;&lt;br&gt;{0:Copyright}&lt;br&gt;&lt;br&gt;{0:Description}" />
      </Items>
    </DetailView>
    <DetailView Id="LogonAttemptsAmountedToLimit_DetailView" Caption="Đăng nhập">
      <Items>
        <StaticText Id="LogonText" Text="Sau ba lần đăng nhập không thành công , trình duyệt sẽ được đóng lại. Vui lòng liên hệ với một quản trị viên về các thông tin đăng nhập chi tiết của bạn." />
      </Items>
    </DetailView>
    <DetailView Id="ParameterlessLogonFailedInfo_DefaultDetailView" Caption="Đăng nhập">
      <Items>
        <StaticText Id="Message" Text="{0} Vui lòng liên hệ với một quản trị viên về các thông tin chi tiết đăng nhập của bạn." />
      </Items>
    </DetailView>
  </Views>
</Application>