      <RuleFromBoolProperty MessageTemplateMustBeTrue="{TargetPropertyName}&quot; phải được thiết lập là true.&quot;" />
      <RuleIsReferenced MessageTemplateMustBeReferenced="Đối tượng {TargetObject}&quot; phải được tham chiếu.&quot;" MessageTemplateFoundObjects="Các đối tượng sau được tìm thấy: " />
      <RuleObjectExists MessageTemplateMustExist="Các đối tượng thỏa mãn biểu thức lọc {Criteria}&quot; phải tồn tại.&quot;" MessageTemplateFoundObjects="Các đối tượng sau được tìm thấy: " />
