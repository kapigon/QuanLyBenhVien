      <RuleRequiredField MessageTemplateMustNotBeEmpty="{TargetPropertyName}&quot; không được trống.&quot;" />
      <RuleSearchObject MessageTemplateFoundObjects="Các đối tượng sau được tìm thấy: " />
      <RuleStringComparison MessageTemplateMustContain="{TargetPropertyName}&quot; phải chứa &quot;{OperandValue}&quot;.&quot;" MessageTemplateMustBeginWith="{TargetPropertyName}&quot; phải bắt đầu với &quot;{OperandValue}&quot;.&quot;" MessageTemplateMustEndWith="{TargetPropertyName}&quot; phải kết thúc với &quot;{OperandValue}&quot;.&quot;" MessageTemplateMustBeEqual="{TargetPropertyName}&quot; phải bằng &quot;{OperandValue}&quot;.&quot;" MessageTemplateMustNotBeEqual="{TargetPropertyName}&quot; phải không bằng &quot;{OperandValue}&quot;.&quot;" />
